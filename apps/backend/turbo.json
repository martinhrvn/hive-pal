{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["//"],
  "tasks": {
    "db:up:test": {
    },
    "build": {
      "dependsOn": ["^build", "prisma:generate"],
      "outputs": ["dist/**"],
      "cache": true
    },
    "prisma:generate": {
      "inputs": ["prisma/schema.prisma"],
      "outputs": ["node_modules/@prisma/client"],
      "cache": true
    },
    "test:e2e": {
      "dependsOn": ["db:up:test"]
    }
  }}