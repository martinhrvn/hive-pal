{
  "name": "beekeeper",
  "private": true,
  "scripts": {
    "dev": "pnpm --parallel -r dev",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "format": "pnpm -r format && prettier --write \"*.{js,json,md}\"",
    "format:check": "pnpm -r exec prettier --check \"src/**/*.{ts,tsx}\" && prettier --check \"*.{js,json,md}\""
  },
  "packageManager": "pnpm@9.6.0",
  "devDependencies": {
    "turbo": "^2.4.4"
  }
}
